<%- include('./partials/head') %>

<main>


<section class="intro">
    <% tno.forEach(tno => {%>
        <div class="intro-div">
            <h1>Exposense</h1>
            <p><%= tno.intro %></p>
            <p><%= tno.content %></p>

            <%- include('./partials/infographic-svg') %>       
        </div>

    <%}) %>
</section>

<!-- knopje voor popup die wel werkt -->
<div class="center">
    <button id="open-popup" class="btn">+</button>
</div>
<div class="popup" id="popup">
    <div class="overlay"></div>
    <div class="popup-content">
        <div class="controls">
            <button class="close-btn"><%- include('./partials/close-btn') %></button>
        </div>
        <h2>Titel van de popup</h2>
        <p>Content van de popup</p>
    </div>
</div>


</main>

<%- include('./partials/footer') %>
